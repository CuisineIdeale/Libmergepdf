{
    "name": "cideale/libmergepdf",
    "description": "Library for merging multiple PDFs",
    "keywords": ["pdf", "merge"],
    "homepage": "https://github.com/CuisineIdeale/Libmergepdff",
    "type": "library",
    "license": "WTFPL",
    "authors": [
        {
            "name": "CuisineId√©ale",
            "email": "support@cuisineideale.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Cideale\\Libmergepdf\\": "src/"
        },
        "classmap": [
            "tcpdi/"
        ]
    },
    "require": {
        "php": "^7.1||^8.2",
        "tecnickcom/tcpdf": "^6.2.22",
        "setasign/fpdi": "^2"
